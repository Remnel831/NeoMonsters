window/size/viewport_height=900
window/size/resizable=false

[file_customization]

folder_colors={
"res://Assets/": "red",
"res://Scenes/": "purple",
"res://Scripts/": "blue"
}

[global_group]

ball=""

[rendering]

renderer/rendering_method="mobile"
